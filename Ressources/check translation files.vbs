'clascycle + trophées + départsSet fso = CreateObject("Scripting.FileSystemObject")fichierEN = "clascycle.lang.en.txt"Dim TEN()Const ForReading = 1Const TriStateTrue = -1Set f = fso.OpenTextFile(fichierEN, ForReading, False, TriStateTrue)i = 0Do Until f.AtEndOfStream  s = f.ReadLine  if Len(Trim(s)) > 0 Then   t = Split(s,"=")   id = t(0)   Redim preserve TEN(i+1)   TEN(i) = id   i = i + 1  End ifLoopf.CloseSet objFolder = fso.GetFolder(fso.GetParentFolderName(Wscript.ScriptFullName))Set colFiles = objFolder.FilesFor Each objFile in colFiles     strFileName = objFile.Name    If Instr(1,strFileName,"clascycle.lang.") and LCase(strFileName) <> "clascycle.lang.en.txt" Then		Set f = fso.OpenTextFile(objFile.Name, ForReading, False, TriStateTrue)		Dim TEX()		i = 0		Do Until f.AtEndOfStream		  s = f.ReadLine		  if Len(Trim(s)) > 0 Then		   t = Split(s,"=")		   id = t(0)		   Redim preserve TEX(i+1)		   TEX(i) = id		   i = i + 1		  End if		Loop		f.Close				manquants = ""		for j = 0 to ubound(TEN)			ok = false			for k = 0 to ubound(TEX)				if TEX(k) = TEN(j) Then					ok = true					Exit For				end if			next			if not ok then manquants = manquants & vbcrlf & " - " & TEN(j)		next		if manquants = "" then			msgbox "File " & strFileName & " is complete"		else			msgbox "File " & strFileName & " - missing items are (ids):" & manquants		end if    End IfNext